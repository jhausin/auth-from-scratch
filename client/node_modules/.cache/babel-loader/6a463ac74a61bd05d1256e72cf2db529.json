,IAAA,EAAA,IAAA,CAAA,SAAA,CAAA,IAAA,CAFA;AAGA,QAAA,MAAA,EAAA;AACA,0BAAA,kBADA;AAEA,UAAA,aAAA,mBAAA,YAAA,CAAA,OAAA,CAAA,OAAA,CAAA;AAFA;AAHA,OAAA,CAAA,CAQA,IARA,CAQA,UAAA,GAAA;AAAA,eAAA,GAAA,CAAA,IAAA,EAAA;AAAA,OARA,EASA,IATA,CASA,UAAA,IAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,OAXA;AAYA,KAlBA;AAmBA,IAAA,MAnBA,oBAmBA;AACA,MAAA,YAAA,CAAA,UAAA,CAAA,OAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AACA;AAtBA;AAzBA,CAAA","sourcesContent":["<template>\n    <div class=\"home\">\n        <div class=\"jumbotron\">\n            <h1 class=\"display-3 text-center\">\n                Hello, {{ user.username }}! ðŸ‘‹\n            </h1>\n            <hr class=\"my-4\" />\n            <button @click=\"logout()\" class=\"btn btn-primary\">\n                Logout\n            </button>\n            <br />\n            <br />\n            <button @click=\"showForm = !showForm\" class=\"btn btn-info\">\n                Toggle Form\n            </button>\n            <form v-if=\"showForm\" @submit.prevent=\"addNote()\">\n                <div class=\"form-group\">\n                    <label for=\"noteTitle\">Title</label>\n                    <input\n                        v-model=\"newNote.title\"\n                        type=\"text\"\n                        class=\"form-control\"\n                        id=\"noteTitle\"\n                        placeholder=\"Title\"\n                        required\n                    />\n                </div>\n                <div class=\"form-group\">\n                    <label for=\"noteContent\">\n                        Note\n                    </label>\n                    <textarea\n                        v-model=\"newNote.note\"\n                        class=\"form-control\"\n                        id=\"noteContent\"\n                        rows=\"3\"\n                        placeholder=\"Enter your note\"\n                        required\n                    >\n                    </textarea>\n                </div>\n                <button type=\"submit\" class=\"btn btn-primary\">\n                    Add Note\n                </button>\n                <h1>{{ newNote.title }}</h1>\n            </form>\n        </div>\n    </div>\n</template>\n\n<script>\nconst API_URL = \"http://localhost:5000\";\nexport default {\n    data: () => ({\n        showForm: false,\n        newNote: {\n            title: \"\",\n            note: \"\",\n        },\n        user: {},\n    }),\n    mounted() {\n        fetch(API_URL, {\n            headers: {\n                authorization: `Bearer ${localStorage.getItem(\"token\")}`,\n            },\n        })\n            .then((res) => res.json())\n            .then((result) => {\n                if (result.user) {\n                    this.user = result.user;\n                } else {\n                    localStorage.removeItem(\"token\");\n                    this.$router.push(\"/login\");\n                }\n            });\n    },\n    methods: {\n        addNote() {\n            const body = {\n                title: this.newNote.title,\n                note: this.newNote.note,\n            };\n            fetch(\"http://localhost:5000/api/v1/notes/\", {\n                method: \"POST\",\n                body: JSON.stringify(body),\n                header: {\n                    \"content-type\": \"application/json\",\n                    authorization: `Bearer ${localStorage.getItem(\"token\")}`,\n                },\n            })\n                .then((res) => res.json())\n                .then((note) => {\n                    console.log(note);\n                });\n        },\n        logout() {\n            localStorage.removeItem(\"token\");\n            this.$router.push(\"/login\");\n        },\n    },\n};\n</script>\n\n<style></style>\n"],"sourceRoot":"src/views"}]}